{
  "name": "svg-insert",
  "version": "0.1.0",
  "engines": {
    "sketch": ">=3.0"
  },
  "skpm": {
    "name": "SVG-Insert",
    "manifest": "src/manifest.json",
    "main": "SVG-Insert.sketchplugin",
    "assets": [
      "assets/**/*"
    ]
  },
  "scripts": {
    "build": "skpm-build",
    "watch": "skpm-build --watch",
    "start": "skpm-build --watch --run",
    "postinstall": "npm run build && skpm-link"
  },
  "devDependencies": {
    "@skpm/builder": "^0.4.0"
  },
  "author": "tankxu",
  "description": "Insert SVG code as a Layer.",
  "homepage": "https://github.com/tankxu/SVG-Insert",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tankxu/SVG-Insert.git"
  }
}
